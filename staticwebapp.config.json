{
	"routes": [{
		"route": "/*",
		"allowedRoles": ["authenticated"]
	}],
	"navigationFallback": {
		"rewrite": "/index.html"
	},
	"networking": {
		"allowedIpRanges": ["AzureFrontDoor.Backend",
			"AzureActiveDirectory.Backend"
		]
	},
	"responseOverrides": {
		"404": {
			"rewrite": "/auth.html"
		}
	}
}
